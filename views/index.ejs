    <div class="navbar">
        <a href="/home">Home</a>
        <a href="/article/new">Add-Product</a>
        <a href="/profile/User">Profile</a>
    </div>
    <div class="search-container">
        <form action="/search" method="post">
            <input type="text" name="keyi" placeholder="Search...." required>
            <button type="submit">search</button>
        </form>
    </div>
    <div class="heading2">
        <h1>Available-Product</h1>
    </div>

    <div class="blog">
        <% article.forEach(element=> { %>


            <div class="card">
                <img src="<%=element.productimg.url%>" alt="Error" style="width:100%">
                <div class="prcon">
                    <h5>
                        <%= element.title %>
                    </h5>
                    <p class="price">Price:<%= element.price %></p>
                    <!-- <h5><i>CreatedBy:<%=element.createdname %></i></h5> -->
                    <div class="buttons">
                         <a class="viewmore" href="/articleview/<%=element.slug %>">View More</a>
                    </div>
                </div>
               
               

            </div>


            <% }); %>
    </div>
